 <%- include('../../partials/header') %>

	<div class="container col-md-8 col-xl-6 mt-5">
		<div class="container mt-2">
			<div class="container d-flex flex-column mb-3">
				<div class="align-self-center">
					<h2 class="text-capitalize">
						<%=admin.fullName %> Dashboard
					</h2>
				</div>
			</div>
			<% var n=0;%>
			<div class="container d-flex flex-column">
			<% dashboard.forEach((index)=>{ %>
				<% n++;%>
				<% if(index.priviledge <= admin.priviledge){ %>
				<div id='dropdowndiv'>
					<div class="container d-flex flex-row justify-content-sm-between border border-secondary shadow rounded mt-3 my-1" id="dashmod-bg">
					<div class="container d-flex flex-column">
						<div class="my-auto">
							<h6>
							<%=index.module%>
							</h6>
						</div>
					</div>
					<div class="container d-flex flex-column ">
						<div class=" d-none d-lg-inline ">
							<div class="align-self-center my-1">
								<p class="text-wrap my"><%=index.description%></p>
							</div>
						</div>
					</div>
					<div class="container d-flex flex-column">
						<div class="align-self-end my-auto">
							<a class="btn btn-success shadow my-3" data-toggle="collapse" data-target="#dropdashdiv_<%=n%>" id="dashexp">Expand</a>
						</div>
					</div>
				</div>
				<div class="collapse" id="dropdashdiv_<%=n%>">
					<% index.ref.forEach((refindex)=>{%>
					<div class="d-flex flex-column mx-auto col-lg-10">
						<div class="container d-flex justify-content-between border border-secondary rounded my-1">
					<div class="container d-flex flex-column">
						 <div class="my-auto">
							 <h6>
								<%= refindex.name %>
							 </h6>
						 </div>
						 </div>	
					 <div class="container d-flex flex-column">
						 <div class="align-self-center my-1">
							 <a class="btn btn-success" id="launch_btn" href="<%=refindex.path%>">Launch</a>
						 	</div>
						 </div>
						</div>
					</div>
					<% }); %>
				</div>
				</div>

			<% } } ) %>
			</div>
		</div>
	</div>

<script src="/sc_static/scripts/dashboard.js" type="text/javascript"></script>

<%- include('../../partials/footer') %>